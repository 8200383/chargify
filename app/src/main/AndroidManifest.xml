<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Chargify"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Chargify">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".features.chargers.ChargersScreen"
            android:label="Chargers" />
        <activity
            android:name=".features.stations.StationsActivity"
            android:label="Timeline" />
        <activity
            android:name=".features.bookmarks.BookmarksActivity"
            android:label="Timeline" />
        <activity
            android:name=".features.timeline.TimelineActivity"
            android:label="Timeline" />
    </application>

</manifest>